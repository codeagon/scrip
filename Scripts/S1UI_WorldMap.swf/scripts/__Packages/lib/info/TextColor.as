   static function setWorldMapTextColor(isOver, type, targetTF)
   {
      var _loc2_ = new TextFormat();
      if(isOver)
      {
         if(type == "world")
         {
            targetTF._alpha = 100;
            targetTF.textColor = lib.info.TextColor.WORLDMAP_WORLD_OVER;
            targetTF.setTextFormat(_loc2_);
         }
         else if(type == "guard")
         {
            _loc2_.alpha = 100;
            _loc2_.shadowAlpha = 100;
            targetTF.textColor = lib.info.TextColor.WORLDMAP_GUARD_OVER;
            targetTF.setTextFormat(_loc2_);
         }
         else if(type == "unexplored")
         {
            _loc2_.alpha = 100;
            _loc2_.shadowAlpha = 100;
            targetTF.textColor = lib.info.TextColor.WORLDMAP_GUARD_UNEXPLORED;
            targetTF.setTextFormat(_loc2_);
         }
         else if(type == "village")
         {
            _loc2_.alpha = 100;
            _loc2_.shadowAlpha = 100;
            targetTF.textColor = lib.info.TextColor.WORLDMAP_VILLAGE_OVER;
            targetTF.setTextFormat(_loc2_);
         }
         else if(type == "outside")
         {
            targetTF.textColor = lib.info.TextColor.WORLDMAP_OUTSIDE_OVER;
         }
         else if(type == "dungeon")
         {
            targetTF.textColor = lib.info.TextColor.WORLDMAP_DUNGEON_OVER;
         }
      }
      else if(type == "world")
      {
         targetTF._alpha = 100;
         targetTF.textColor = lib.info.TextColor.WORLDMAP_WORLD_OUT;
         targetTF.setTextFormat(_loc2_);
      }
      else if(type == "world_continental")
      {
         targetTF.shadowColor = 7824718;
         targetTF.shadowBlurX = targetTF.shadowBlurY = 10;
         targetTF.shadowStrength = 2;
         targetTF.shadowQuality = 2;
         targetTF.shadowDistance = 0;
         targetTF._alpha = 100;
         targetTF.textColor = lib.info.TextColor.WORLDMAP_WORLD_CONTINENTAL;
         targetTF.setTextFormat(_loc2_);
      }
      else if(type == "guard")
      {
